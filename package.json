{
  "name": "pnut-cli",
  "version": "1.0.4",
  "description": "快速生成项目模板的脚手架",
  "keywords": ["boilerplate", "vue", "cli", "template"],
  "author": "yuzheng",
  "bin": {
    "pnut-cli": "outfile.cjs"
  },
  "files": [
    "outfile.cjs",
    "template"
  ],
  "scripts": {
    "dev": "esno index.ts",
    "build": "zx ./scripts/build.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yuzhengbest/rs-cli"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": [
      "prettier --write --cache"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@tsconfig/node18": "^18.2.2",
    "@types/eslint": "^8.44.4",
    "@types/minimist": "^1.2.4",
    "@types/node": "^18.18.5",
    "@vue/create-eslint-config": "^0.3.2",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "ejs": "^3.1.9",
    "esbuild": "^0.18.20",
    "esbuild-plugin-license": "^1.2.2",
    "esno": "^0.17.0",
    "husky": "^8.0.3",
    "kolorist": "^1.8.0",
    "lint-staged": "^15.0.1",
    "minimist": "^1.2.8",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "prompts": "^2.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "zx": "^7.2.3"
  },
  "license": "MIT"
}
